{
  "name": "munizeverton/defender",
  "description": "Fork de artesaos/defender",
  "license": "MIT",
  "require": {
    "php": ">=5.6.4",
    "illuminate/contracts": "~5.4",
    "illuminate/http": "~5.4",
    "illuminate/support": "~5.4",
    "illuminate/database": "~5.4",
    "illuminate/view": "~5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "phpspec/phpspec": "~2.1",
    "friendsofphp/php-cs-fixer": "~1.0",
    "orchestra/testbench": "~3.4@dev",
    "orchestra/database": "~3.4@dev",
    "fzaninotto/faker": "~1.5"
  },
  "autoload": {
    "psr-4": {
      "Munizeverton\\Defender\\": "src/Defender/"
    }
  },
  "config":{
    "preferred-install": "dist"
  },
  "minimum-stability" : "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Artesaos\\Defender\\Providers\\DefenderServiceProvider"
      ],
      "aliases": {
        "Defender": "Artesaos\\Defender\\Facades\\Defender"
      }
    }
  }
}
